{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "typeRoots": ["./node_modules/@types", "./types"],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(booking)/booking/page.jsx",
    "app/extras/page.jsx",
    "app/(home)/Vehicles.jsx",
    "app/(admin)/admin/dashboard",
    "middleware.js",
    "app/(client)/(home)/Vehicles.tsx"
, "app/(client)/(extras)/extras/components/PriceCalculator"  ],
  "exclude": ["node_modules"]
}
